# Production Environment Configuration
# Copy this file to .env.production and fill in the values

# API Gateway URL (get from Terraform output: terraform output api_gateway_url)
VITE_API_URL=https://xxxxxxxxxx.execute-api.il-central-1.amazonaws.com/prod

# AWS Cognito Configuration (get from Terraform outputs)
VITE_COGNITO_REGION=il-central-1
VITE_COGNITO_USER_POOL_ID=il-central-1_xxxxxxxxx
VITE_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# WebSocket URL for real-time updates (if different from API URL)
VITE_WEBSOCKET_URL=wss://xxxxxxxxxx.execute-api.il-central-1.amazonaws.com/prod

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ML_PREDICTIONS=true

# Environment
VITE_ENVIRONMENT=production
