# Asterisk Worker Configuration

# Server Configuration
PORT=3000
NODE_ENV=production

# Asterisk Manager Interface (AMI)
AMI_HOST=localhost
AMI_PORT=5038
AMI_USERNAME=admin
AMI_PASSWORD=your-ami-password

# Asterisk REST Interface (ARI)
ARI_HOST=localhost
ARI_PORT=8088
ARI_USERNAME=asterisk
ARI_PASSWORD=your-ari-password

# AWS Configuration
AWS_REGION=il-central-1
S3_AUDIO_BUCKET=your-audio-bucket-name

# SNS Topics
SNS_CALL_EVENTS_TOPIC=arn:aws:sns:il-central-1:123456789012:mass-voice-campaign-call-events-dev
SNS_DONATION_EVENTS_TOPIC=arn:aws:sns:il-central-1:123456789012:mass-voice-campaign-donation-events-dev
SNS_OPTOUT_EVENTS_TOPIC=arn:aws:sns:il-central-1:123456789012:mass-voice-campaign-optout-events-dev
